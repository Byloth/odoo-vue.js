<odoo>

    <template id="webclient_bootstrap" inherit_id="web.webclient_bootstrap">
        <xpath expr="//t[@t-call-assets='web.assets_common']" position="before">
            <script type="text/javascript" src="https://fb.me/react-15.0.1.js"/>
            <script type="text/javascript" src="https://fb.me/react-dom-15.0.1.js"/>
            <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/babel-core/5.8.23/browser.min.js"/>
        </xpath>
    </template>

    <template id="assets_backend" inherit_id="web.assets_backend">
        <xpath expr=".">
            <script type="text/javascript" src="/web_react/static/src/js/react_view.js"/>
        </xpath>
    </template>

</odoo>
